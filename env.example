# Configuración de Base de Datos
DB_HOST=localhost
DB_NAME=canchas_db
DB_USER=root
DB_PASS=

# Configuración de la Aplicación
APP_ENV=local
BASE_URL=http://localhost:8000

# JWT Secret (generar una clave segura)
